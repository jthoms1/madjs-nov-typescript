{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2015",
    "lib":[
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "sourceMap": false
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
